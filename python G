# setup.py - Basic setup script for a Python repository

from setuptools import setup, find_packages

setup(
    name='my_project',
    version='1.0.0',
    author='Teto',
    description='A cybersecurity tool for detecting phishing URLs',
    packages=find_packages(),
    install_requires=[
        'requests',
        'beautifulsoup4',
        'scikit-learn',
        'pandas'
    ],
    entry_points={
        'console_scripts': [
            'phish-detector=src.main:run'
        ]
    },
    classifiers=[
        'Programming Language :: Python :: 3',
        'License :: OSI Approved :: MIT License',
        'Operating System :: OS Independent',
    ],
)


